{
  "name": "react-dynamic-geometry",
  "version": "0.0.9",
  "repository": "https://github.com/gksander/react-dynamic-geometry.git",
  "author": "Grant Sander <gksander93@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "tsup src/index.ts --dts --format cjs,esm --external react",
    "dev": "tsc --watch",
    "playground": "parcel playground/index.html",
    "prepare": "yarn build",
    "test": "jest --config=jest.config.js",
    "test:watch": "jest --config=jest.config.js --watch",
    "prepublishOnly": "yarn build",
    "preversion": "yarn test"
  },
  "devDependencies": {
    "@babel/core": "^7.13.8",
    "@babel/preset-env": "^7.13.9",
    "@babel/preset-typescript": "^7.13.0",
    "@swc/core": "^1.3.24",
    "@testing-library/react": "^11.2.5",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.31",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "babel-jest": "^26.6.3",
    "jest": "^26.6.3",
    "parcel-bundler": "1.12.3",
    "prettier": "^2.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "ts-jest": "^26.5.3",
    "tsup": "^6.5.0",
    "typescript": "^4.2.3"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": [
      {
        "import": "./dist/index.mjs",
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "./dist/index.js"
    ],
    "./package.json": "./package.json"
  },
  "dependencies": {
    "jotai": "^0.15.1"
  }
}
